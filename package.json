{
  "name": "discordjs-template",
  "version": "1.0.0",
  "description": "A basic template for building a discord bot using the latest version of discord.js and typescript.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "dev": "tsup --watch --onSuccess 'node .'",
    "lint": "eslint src",
    "build": "pnpm lint && tsup"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "tsup": "^6.5.0",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1"
  }
}
